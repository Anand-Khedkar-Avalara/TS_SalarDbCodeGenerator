		<TextFieldIdenticator>text</TextFieldIdenticator>
		<ArrayIdenticator>[]</ArrayIdenticator>
		<DbDecimalName>decimal</DbDecimalName>
		<DbNumericName>numeric</DbNumericName>
		<DbDecimalType>decimal([:Precision:],[:Scale:])</DbDecimalType>
		<DbNumericType>numeric([:Precision:],[:Scale:])</DbNumericType>
		<LanguageKeywordsSuppress>{0}_{1}</LanguageKeywordsSuppress>
