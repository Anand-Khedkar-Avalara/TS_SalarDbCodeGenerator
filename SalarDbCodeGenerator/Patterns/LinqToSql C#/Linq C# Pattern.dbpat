<?xml version="1.0" encoding="utf-8"?>
<PatternProject xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Name>LinqToSql C#</Name>
	<Description>This pattern generates object relational database model based on LinqToSql.</Description>
	<Author>SalarSoft</Author>
	<LastUpdate>2010/04/09</LastUpdate>
	<Language>C#</Language>
	<FileExtension>.cs</FileExtension>
	<SupportedDatabases>MS SQL Server</SupportedDatabases>
	<SeperateRefOtherColumns>false</SeperateRefOtherColumns>
	<LanguageSettings>
