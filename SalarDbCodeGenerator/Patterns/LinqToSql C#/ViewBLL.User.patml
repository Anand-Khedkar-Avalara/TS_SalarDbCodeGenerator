<?xml version="1.0" encoding="utf-8" ?>
<pattern>
	<Name>User View BLL</Name>
	<Description>Busines logic layer for views user custom methods</Description>
	<Options
		Group="ViewBLL"
		AppliesTo="Views_Each"
		Overwrite="False"
		FilePath="BLL\[:TableName:]BLL.cs"
		Language="C#"
		/>
	<BaseContent>
		<![CDATA[using System;
using System.Collections.Generic;
using System.Text;
using [:Namespace:].Base;
using [:Namespace:].Entities;

namespace [:Namespace:].BLL
{
	/// <summary>
	/// User custom methods for [:TableNativeName:]
	/// </summary>
	partial class [:TableName:]BLL
	{

	}
}
]]>
	</BaseContent>
</pattern>